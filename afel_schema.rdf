	<!-- Define the properties for the different classes -->

	<!-- Define the properties for the Artifact class -->
	<rdf:Property rdf:about="resourceID">
		<rdfs:label>Resource Identifier</rdfs:label>
		<rdfs:domain rdf:resource="Artifact"/>
		<rdfs:range rdf:resource="xsd:integer">
	</rdf:Property>

	<rdf:Property rdf:about="URL">
		<rdfs:label>URL</rdfs:label>
		<rdfs:domain rdf:resource="Artifact"/>
		<rdfs:range rdf:resource="xsd:String"/>
		<owl:equivalentProperty rdf:resource="schema:url" />
	</rdf:Property>

	<rdf:Property rdf:about="content">
		<rdfs:label>Content</rdfs:label>
		<rdfs:domain rdf:resource="Artifact"/>
		<rdfs:range rdf:resource="xsd:String"/>
		<owl:equivalentProperty rdf:resource="schema:text" />
	</rdf:Property>

	<rdf:Property rdf:about="date">
		<rdfs:label>Date Created</rdfs:label>
		<rdfs:domain rdf:resource="Artifact"/>
		<rdfs:range rdf:resource="xsd:date"/>
		<owl:equivalentProperty rdf:resource="schema:dateCreated" />
	</rdf:Property>

	<rdf:Property rdf:about="creator">
		<rdfs:label>Creator</rdfs:label>
		<rdfs:domain rdf:resource="Artifact"/>
		<rdfs:range>
			<owl:Class>
					<owl:unionOf rdf:parseType="Collection">
						<owl:Class rdf:about="Person"/>
						<owl:Class rdf:about="Organization"/>
					</owl:unionOf>
			</owl:Class>
		</rdfs:range>
		<owl:equivalentProperty rdf:resource="schema:creator" />
	</rdf:Property>

	<rdf:Property rdf:about="contributor">
		<rdfs:label>Contributor</rdfs:label>
		<rdfs:domain rdf:resource="Artifact"/>
		<rdfs:range>
			<owl:Class>
					<owl:unionOf rdf:parseType="Collection">
						<owl:Class rdf:about="Person"/>
						<owl:Class rdf:about="Organization"/>
					</owl:unionOf>
			</owl:Class>
		</rdfs:range>
		<owl:equivalentProperty rdf:resource="schema:contributor" />
	</rdf:Property>

	<rdf:Property rdf:about="revisionNumber">
		<rdfs:label>Revision Number</rdfs:label>
		<rdfs:domain rdf:resource="Artifact"/>
		<rdfs:range rdf:resource="xsd:long"/>
